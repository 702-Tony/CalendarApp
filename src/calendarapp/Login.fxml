<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="367.0" prefWidth="530.0" style="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="calendarapp.Login">
  <center>
    <VBox alignment="CENTER" prefHeight="367.0" prefWidth="377.0">
      <children>
        <Label fx:id="label" contentDisplay="CENTER" text="Office Calendar">
          <font>
            <Font name="System Bold" size="30.0" />
          </font>
          <VBox.margin>
            <Insets bottom="20.0" />
          </VBox.margin>
        </Label>
        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="200.0">
          <children>
            <GridPane prefWidth="242.0">
              <children>
                <TextField fx:id="usernameField" alignment="CENTER_LEFT" maxWidth="-Infinity" prefWidth="200.0" promptText="Username" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <Label fx:id="userLabel" text="User" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <Label fx:id="passLabel" text="Pass" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <PasswordField fx:id="passwordField" alignment="CENTER_LEFT" maxWidth="-Infinity" prefWidth="200.0" promptText="Password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
              </children>
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="99.0" minWidth="10.0" prefWidth="47.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="153.0" minWidth="10.0" prefWidth="153.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
          </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="200.0">
          <children>
            <Button fx:id="login" defaultButton="true" minHeight="-1.0" mnemonicParsing="false" onAction="#handleLogin" onKeyPressed="#handleLogin" text="Log In">
              <HBox.margin>
                <Insets left="5.0" right="5.0" top="5.0" fx:id="x1" />
              </HBox.margin>
            </Button>
            <Button fx:id="cancel" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" HBox.margin="$x1" />
          </children>
        </HBox>
      </children>
    </VBox>
  </center>
</BorderPane>
